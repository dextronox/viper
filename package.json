{
  "name": "viper",
  "repository": {
    "type": "git",
    "url": "https://github.com/dextronox/viper.git"
  },
  "version": "1.0.0",
  "description": "Viper VPN Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:win32": "electron-packager . Viper --platform=win32 --arch=ia32 --executable-name=Viper --win32metadata.FileDescription=Viper --icon=icons/icon.ico --ignore=log.txt --ignore=current_user.txt --ignore=current_login.txt --ignore=current_vpn.ovpn --out dist/",
    "pack:win64": "electron-packager . Viper --platform=win32 --arch=x64 --executable-name=Viper --win32metadata.FileDescription=Viper --icon=icons/icon.ico --ignore=log.txt --ignore=current_user.txt --ignore=current_login.txt --ignore=current_vpn.ovpn --out dist/",
    "installer:win64": "electron-installer-windows --config dist/config_x64.json",
    "installer:win32": "electron-installer-windows --config dist/config_ia32.json"
  },
  "author": "Daniel Jennings",
  "license": "MIT License",
  "devDependencies": {
    "electron": "^2.0.4",
    "electron-winstaller": "^2.6.4"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "child_process": "^1.0.2",
    "electron-log": "^2.2.16",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "jquery": "^3.3.1",
    "node-cmd": "^3.0.0",
    "os": "^0.1.1",
    "popper.js": "^1.14.3",
    "request": "^2.87.0",
    "request-progress": "^3.0.0",
    "shell": "^0.5.0",
    "sudo-prompt": "^8.2.0",
    "sweetalert": "^2.1.0",
    "update-electron-app": "^1.3.0"
  }
}
